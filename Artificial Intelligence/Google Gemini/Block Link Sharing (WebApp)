// Block Link Sharing (WebApp)
IF (MWG.SmartMatch (MWG.Url, "gemini.google.com/_/BardChatUi/data/batchexecute") AND MWG.HasParameter (MWG.Url, "rpcids") == TRUE AND EqualsCaseInsensitive (MWG.Parameter (MWG.Url, "rpcids"), "fuVx7")) THEN {
  MWG.Block (McAfee_Blocked_by_Application_Control, "Block Link Sharing (WebApp)", "Google Gemini Controls")
}
